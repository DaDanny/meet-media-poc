{
  "name": "meet-transcription-poc",
  "version": "1.0.0",
  "description": "Real-time transcription POC for Google Meet 1:1 meetings",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "npm run build:server && npm run build:web",
    "build:server": "tsc",
    "build:web": "webpack --mode production",
    "build:dev": "tsc && webpack --mode development",
    "test": "jest"
  },
  "keywords": [
    "google-meet",
    "transcription",
    "speech-to-text",
    "webrtc",
    "real-time"
  ],
  "author": "Your Name",
  "license": "Apache-2.0",
  "dependencies": {
    "@google-cloud/speech": "^6.6.0",
    "express": "^4.19.2",
    "ws": "^8.18.0",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.4.5",
    "uuid": "^10.0.0",
    "googleapis": "^142.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "@types/express": "^4.17.21",
    "@types/ws": "^8.5.10",
    "@types/cors": "^2.8.17",
    "@types/uuid": "^10.0.0",
    "typescript": "^5.5.4",
    "ts-node-dev": "^2.0.0",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "ts-loader": "^9.5.1",
    "html-webpack-plugin": "^5.6.0",
    "@webpack-dev-server/client": "^4.15.2",
    "webpack-dev-server": "^4.15.2",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.12"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
